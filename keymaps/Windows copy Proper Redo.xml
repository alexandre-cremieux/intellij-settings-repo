<keymap version="1" name="Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="SelectIn">
    <keyboard-shortcut first-keystroke="alt f1" />
    <keyboard-shortcut first-keystroke="ctrl #10000b2" />
  </action>
  <action id="ToggleInlineHintsAction">
    <keyboard-shortcut first-keystroke="shift alt h" />
  </action>
</keymap>